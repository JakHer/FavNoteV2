{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],

  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },

  "rules": {
    "react/jsx-filename-extension": [
      0,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "no-console": "off"
  }
}
